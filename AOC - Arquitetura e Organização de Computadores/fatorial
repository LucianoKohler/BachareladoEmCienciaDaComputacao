.text
.globl main

main:
	li $v0, 5          
	syscall            # scanf n

	move $t0, $v0      # t0 = n
	li $t1, 1          # t1 = 1
                           
loop:
	mult $t1, $t0      # t1 * t0
	mflo $t1           # resultado em t1
	addi $t0, $t0, -1  # t0--
	bgtz $t0, loop     # t0 > 0 ? loop : continue
	
	move $a0, $t1      # a0 = t1
	li $v0, 1          # print a0
	syscall

end:
	li $v0, 10         # fim
	syscall